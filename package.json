    "@babel/preset-react": "7.0.0-beta.54",
    "eslint": "5.2.0",
    "eslint-config-henribeck": "5.2.1",
    "flow-bin": "0.77.0",
    "flow-copy-source": "2.0.2",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "2.5.5",
    "prop-types": "15.6.1",
    "react-display-name": "0.2.3"
